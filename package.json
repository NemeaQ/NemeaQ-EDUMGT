{
  "private": true,
  "name": "obsidianorder",
  "version": "1.0.0",
  "description": "Education Organization Management System",
  "main": "index.js",
  "devDependencies": {
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "postcss-csso": "^6.0.0",
    "postcss-import": "^14.0.2",
    "postcss-media-minmax": "^5.0.0",
    "uglify-js": "^3.14.5"
  },
  "dependencies": {
    "bootstrap": "^5.1.3",
    "chart.js": "^3.6.2",
    "milligram": "^1.4.1"
  },
  "scripts": {
    "browserify": "browserify src/js/script.js -o src/js/bundle.js",
    "uglifyjs": "uglifyjs src/js/bundle.js -o src/js/bundle.min.js -c -m",
    "postcss": "npx postcss src/css/index.css --use postcss-import --use postcss-media-minmax --use postcss-csso --no-map --output src/css/index.min.css",
    "build": "npm run browserify && npm run uglifyjs && npm run postcss",
    "dev": "npx postcss src/css/index.css --use postcss-import --use postcss-media-minmax --use postcss-csso --no-map --output src/css/index.min.css"
  },
